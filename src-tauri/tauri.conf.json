  "plugins": {
    "updater": {
      "active": true,
      "createUpdaterArtifacts": true,
      "endpoints": [
        "https://github.com/rhino78/payrollapp/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQxMDREQThDRTc1RENFNzgKUldSNHpsM25qTm9FMGF6dmd3NnVQK24zd3pEVFpXT1Ixd3lUSjFQdGwzWGE1eGZwbDVSeEp5SEcK"
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Medical Arts Pharmacy Payroll",
        "width": 1500,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": ["msi"],
